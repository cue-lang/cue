{
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(go test:*)",
      "Bash(go build:*)",
      "Bash(go list:*)",
      "Bash(go fmt:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(ls:*)",
      "Bash(tail:*)",
      "Bash(awk:*)",
      "Bash(git status:*)",
      "Bash(git log:*)",
      "Bash(git show:*)",
      "Bash(git diff:*)",
      "Bash(git branch:*)",
      "Bash(git ls-files:*)",
      "Bash(git ls-tree:*)",
      "Bash(git rev-parse:*)",
      "Bash(git describe:*)",
      "Bash(git config --get:*)",
      "Bash(git config --list:*)",
      "Bash(git add:*)",
      "Bash(git gofmt:*)",
      "Bash(true:)",
      "Bash(echo:*)",
      "Bash(go run:./cmd/cue eval*)",
      "Bash(go run:./cmd/cue export*)",
      "Bash(go run:./cmd/cue def*)",
      "Bash(gh pr view:*)",
      "Bash(gh issue view:*)",
      "Bash(gh pr list:*)",
      "Bash(gh issue list:*)",
      "Bash(gh repo view:*)",
      "Bash(gh pr status:*)",
      "Bash(gh issue status:*)",
      "Bash(gh pr diff:*)",
      "Bash(gh pr checks:*)",
      "Bash(gh workflow view:*)",
      "Bash(gh workflow list:*)",
      "Bash(gh run view:*)",
      "Bash(gh run list:*)",
      "Bash(gh api:*)",
      "Bash(CUE_UPDATE=1 go test:*)",
      "Bash(go test -run:*)",
      "Bash(go test -race:*)",
      "Bash(go vet:*)"
    ],
    "deny": [
      "Bash(go:* -exec*)",
      "Bash(go:* -toolexec*)"
    ]
  }
}