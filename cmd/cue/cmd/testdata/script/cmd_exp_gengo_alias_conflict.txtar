# Two CUE files in a package import two different packages under the same explicit name.
exec cue exp gengotypes
cmp cue_types_main_gen.go expect-gengo

-- cue.mod/module.cue --
module: "mod.test"
language: version: "v0.13.0"

-- pkg/foo/p1/p1.cue --
package p1

#Foo: int

-- pkg/bar/p2/p2.cue --
package p2

#Bar: string

-- f1.cue --
package main

import (
	same "mod.test/pkg/foo/p1"
)

#A: {
	foo: same.#Foo
}

-- f2.cue --
package main

import (
	same "mod.test/pkg/bar/p2"
)

#B: {
	bar: same.#Bar
}

-- expect-gengo --
// Code generated by "cue exp gengotypes"; DO NOT EDIT.

package main

import (
	same1 "mod.test/pkg/bar/p2"
	same "mod.test/pkg/foo/p1"
)

type A struct {
	Foo same.Foo `json:"foo"`
}

type B struct {
	Bar same1.Bar `json:"bar"`
}
