# File starting with .
exec cue eval .foo.cue
cmp stdout expect-stdout-dot

# File starting with _
exec cue eval _foo.cue
cmp stdout expect-stdout-underscore

# Ensure package eval excludes both for completeness
# TODO fix this
! exec cue eval .
cmp stderr expect-stderr-pkg
# cmp stdout expect-stdout-pkg

-- .foo.cue --
package foo

a: 42
-- _foo.cue --
package foo

b: 42
-- .bar.cue --
package bar
-- foo.cue --
package foo

c: 42
-- expect-stdout-dot --
a: 42
-- expect-stdout-underscore --
b: 42
-- expect-stdout-pkg --
c: 42
-- expect-stderr-pkg --
found packages "bar" (.bar.cue) and "foo" (.foo.cue) in "."
