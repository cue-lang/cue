exec cue fmt ./fmt
cmp fmt/fmt.cue want-fmt.cue

-- want-fmt.cue --
package home

import (
	"foo.example/x"
)

a: x.value
-- fmt/fmt.cue --
package home

import (
"foo.example/x"
)
a:     x.value
-- fmt/cue.mod --
module: "main.example"

deps: "foo.example/x@v0": {
	default: true
	v: "v0.0.1"
}
-- _registry/foo.example_x_v0.0.1/cue.mod/module.cue --
module: "foo.example/x@v0"
-- _registry/foo.example_x_v0.0.1/text.cue --
package x
value: "world"
