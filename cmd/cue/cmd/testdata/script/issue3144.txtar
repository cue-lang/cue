# Check that unrelated invalid files do not cause an error when files (only)
# are specified on the command line.

env CUE_EXPERIMENT=modules
env CUE_CACHE=$WORK/tmp/cache

exec cue export valid.cue
cmp stdout stdout.golden

-- stdout.golden --
{
    "out": "bar"
}
-- cue.mod/module.cue --
module: "foo.test/bar"
language: version: "v0.9.0"
-- valid.cue --
#foo: "bar"
out: #foo
-- invalid.cue --
foo :: "bar"
out: foo
