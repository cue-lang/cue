exec cue import --outfile=- --list                              input-noempty.yaml
cmp stdout output-noempty-list.cue
exec cue import --outfile=-        --path=strings.ToLower(kind) input-noempty.yaml
cmp stdout output-noempty-exprpath.cue
exec cue import --outfile=- --list --path=strings.ToLower(kind) input-noempty.yaml
cmp stdout output-noempty-list-exprpath.cue
exec cue import --outfile=- --list --path=fixedpath:            input-noempty.yaml
cmp stdout output-noempty-list-fixedpath.cue

exec cue import --outfile=- --list                              input-withempty.yaml
cmp stdout output-withempty-list.cue
! exec cue import --outfile=-        --path=strings.ToLower(kind) input-withempty.yaml
cmp stdout output-withempty-exprpath.cue
! exec cue import --outfile=- --list --path=strings.ToLower(kind) input-withempty.yaml
cmp stdout output-withempty-list-exprpath.cue
exec cue import --outfile=- --list --path=fixedpath:            input-withempty.yaml
cmp stdout output-withempty-list-fixedpath.cue

exec cue import --outfile=- --list                              input-withnull.yaml
cmp stdout output-withnull-list.cue
! exec cue import --outfile=-        --path=strings.ToLower(kind) input-withnull.yaml
cmp stdout output-withnull-exprpath.cue
! exec cue import --outfile=- --list --path=strings.ToLower(kind) input-withnull.yaml
cmp stdout output-withnull-list-exprpath.cue
exec cue import --outfile=- --list --path=fixedpath:            input-withnull.yaml
cmp stdout output-withnull-list-fixedpath.cue

exec cue import --outfile=- --list                              input-missing-key.yaml
cmp stdout output-missing-key-list.cue
! exec cue import --outfile=-        --path=strings.ToLower(kind) input-missing-key.yaml
cmp stdout output-missing-key-exprpath.cue
! exec cue import --outfile=- --list --path=strings.ToLower(kind) input-missing-key.yaml
cmp stdout output-missing-key-list-exprpath.cue
exec cue import --outfile=- --list --path=fixedpath:            input-missing-key.yaml
cmp stdout output-missing-key-list-fixedpath.cue
! exec cue import --outfile=-        --path=strings.ToLower(typo) input-missing-key.yaml
cmp stdout output-missing-key-typopath.cue

exec cue import --outfile=- --with-context --path='"number-\(index+1)"' input-withempty.yaml
cmp stdout output-withempty-witcontext-index.cue
! exec cue import --outfile=- --with-context --path='"number-\(index+1)"' --path=data.kind input-withempty.yaml
cmp stdout output-withempty-witcontext-index-kind.cue

