-- x_tool.cue --
package x

import (
        "tool/cli"
        "encoding/yaml"
)

command: dostuff: {
        write: cli.Print & {
                text: "yaml is " + yaml.Marshal(w)
        }
}

command: dostuffloop: {
        for w in l {
                write: cli.Print & {
                        text: "yaml is " + yaml.Marshal(w)
                }
        }
}
