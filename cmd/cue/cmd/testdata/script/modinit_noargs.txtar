# TODO we might want to deprecate or remove the ability to create
# module file with an empty module directive.
env CUE_VERSION_OVERRIDE=v0.1.2
exec cue mod init
cmp cue.mod/module.cue want-module.cue
exists cue.mod/usr
exists cue.mod/pkg

-- want-module.cue --
module: ""
language: {
	version: "v0.1.2"
}
