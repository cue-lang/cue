# This test tests that submodules work OK.

exec cue eval .
cmp stdout expect-stdout
-- expect-stdout --
a: {
    e: true
    f: true
}
-- cue.mod/module.cue --
module: "main.org"

deps: {
	"example.com@v0": {
		default: true
		v: "v0.0.1"
	}
	"example.com/foo@v0": {
		default: true
		v: "v0.0.1"
	}
}
-- main.cue --
package main
import (
	"example.com@v0:example"
	"example.com/foo@v0"
)
a: {
	e: example.e
	f: foo.f
}
