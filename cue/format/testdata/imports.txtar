sort-imports

-- imports.input --
package foo

import (
    "cuelang.org/go/foo"
    "cuelang.org/go/bar"
    "time"
)

import (
    time1 "time"

    // comment f2
    f2 "cuelang.org/go/foo"
    f1 "cuelang.org/go/foo"
)

import (
    time2 "time"

    same "cuelang.org/go/foo" // comment 1
    same2 "cuelang.org/go/foo" // comment 2
)


a: time.time
b: foo.foo
c: bar.Bar
-- imports.golden --
package foo

import (
	"cuelang.org/go/foo"
	"cuelang.org/go/bar"
	"time"
)

import (
	time1 "time"

	// comment f2
	f2 "cuelang.org/go/foo"
	f1 "cuelang.org/go/foo"
)

import (
	time2 "time"

	same "cuelang.org/go/foo"  // comment 1
	same2 "cuelang.org/go/foo" // comment 2
)

a: time.time
b: foo.foo
c: bar.Bar
