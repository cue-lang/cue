#name: recursive closing starting at non-definition
#evalPartial
-- in.cue --
z: a: {
	#B: {
		c: d: 1
		c: f: 1
	}
}
A: z & {a: {#B: {c: e: 2}}}
-- out/compile --
--- in.cue
{
  z: {
    a: {
      #B: {
        c: {
