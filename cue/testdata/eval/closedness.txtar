	and: p1: {
		X: #a & #b // "k" <= x && x <= "m"

		err: X
		err: j: 3

		ok: X
		ok: k: 3
	}

	and: p2: {
		X: #b & #a // "k" <= x && x <= "m"
		err: X
		err: j: 3

		ok: X
		ok: k: 3
	}

	andEmbed: p1: {
		X: { #a & #b } // "k" <= x && x <= "m"

		err: X
		err: j: 3

		ok: X
		ok: k: 3
	}

	andEmbed: p2: {
		X: { #b & #a } // "k" <= x && x <= "m"
		err: X
		err: j: 3

		ok: X
		ok: k: 3
	}

	orEmbed: p1: {
		X: { #a, #b } // "k" <= x && x <= "m"

		ok1: X
		ok1: j: 3

		ok2: X
		ok2: k: 3
	}

	orEmbed: p2: {
		X: { #b, #a } // "k" <= x && x <= "m"
		ok1: X
		ok1: j: 3

		ok2: X
		ok2: k: 3
	}

	andOrEmbed: t1: p1: {
		// ("k" <= x && x <= "m")  || ("w" <= x && x <= "y")
		X: {
			#a & #b
			#c & #d
		}
		err1: X
		err1: j: 3

		ok1: X
		ok1: k: 3
	}

	andOrEmbed: t1: p2: {
		// ("k" <= x && x <= "m")  || ("w" <= x && x <= "y")
		X: {
			#c & #d
			#a & #b
		}
		err1: X
		err1: j: 3

		ok1: X
		ok1: k: 3
	}

	andOrEmbed: t2:{
		// ("k" <= x && x <= "m")  || ("w" <= x && x <= "y") || x == "p"
