// minInDef defines examples where a definition is mixed into an open struct,
// where then this struct refers to a field that is closed by the definition.
mixInDef: {
	refInside: {
		#x: b: c: {d: {}, e: f: {} }
		a: #x
		a: b: c: d: {}
		a: b: c: e: c.d
	}
	refEdge: {
		#x: c: {d: {}, e: f: {} }
		a: #x
		a: c: d: {}
		a: c: e: c.d
	}
	refOutside: {
		#x: c: {d: {}, e: f: {} }
		a: #x
		a: c: d: {}
		a: c: e: a.c.d // crosses definition
	}
}

