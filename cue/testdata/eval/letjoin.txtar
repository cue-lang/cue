issue3591: missing: {
	#Result: [_]: {[string]: string}

	result: #Result
	
	#Input: {
		p: "A" | "B"
	}
	
	_input: [Parm=_]: #Input & {
		p: Parm
	}
	
	_input: {
		A: {}
		B: {}
	}
	
	for _, v in _input {
		let Var1 = v.p
		let Var2 = Var1
	
		let Items = {
			"item": {
				key1: Var2
			}
		}
	
		result: "missing-items-\(v.p)": {
			for s, attr in Items {
				if attr.key1 != _|_ {(s): attr.key1}
			}
		}
	}
}
