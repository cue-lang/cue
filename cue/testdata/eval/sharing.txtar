-- let.cue --
import "list"

issue3903: full: {
	_input: null | *{name: "foo"}

	let _inputList = list.FlattenN([_input], 0)

	_inputByName: {
		for input in _inputList {
			(input.name): input
		}
	}

	for name, _input in _inputByName {
		(name): _template & {
			input: _input
		}
	}
	_template: {
		input: name: string
		result: "result-\(input.name)"
	}
}
issue3903: noshare: {
	_input: null | *{name: "foo"}

	let _inputList = list.FlattenN([_input], 0)

	_inputByName: {
		for input in (_inputList & __no_sharing) {
			(input.name): input
		}
	}
	for name, _input in _inputByName {
		(name): _template & {
			input: _input
		}
	}
	_template: {
		input:  name: string
		result: "result-\(input.name)"
	}
}
