#name: referencing field in field comprehension
#evalFull
-- in.cue --
a: {b: c: 4}
a: {
	b: d: 5
	for k, v in b {
		"\(k)": v
	}
}
-- out/compile --
--- in.cue
{
  a: {
    b: {
      c: 4
    }
  }
  a: {
    b: {
      d: 5
    }
    for k, v in 〈0;b〉 {
      "\(〈1;k〉)": 〈1;v〉
    }
  }
}
-- out/eval --
(struct){
  a: (struct){
    b: (struct){
      c: (int){ 4 }
      d: (int){ 5 }
    }
    c: (int){ 4 }
    d: (int){ 5 }
  }
}
