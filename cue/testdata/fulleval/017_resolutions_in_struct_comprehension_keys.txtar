#name: resolutions in struct comprehension keys
#evalFull
-- in.cue --
a: {for _, b in ["c"] {"\(b+".")": "a"}}
-- out/compile --
--- in.cue
{
  a: {
    for _, b in [
      "c",
    ] {
      "\((〈1;b〉 + "."))": "a"
    }
  }
}
-- out/eval --
(struct){
  a: (struct){
    "c.": (string){ "a" }
  }
}
