#name: label and field aliases
#evalFull
-- in.cue --
p: [ID=string]: {name: ID}
A="foo=bar": "str"
a:           A
B=bb:        4
b1:          B
b1:          bb
C="\(a)":    5
c:           C
-- out/compile --
--- in.cue
{
  p: {
    [string]: {
      name: 〈1;-〉
    }
  }
  "foo=bar": "str"
  a: 〈0;"foo=bar"〉
  bb: 4
  b1: 〈0;bb〉
  b1: 〈0;bb〉
  "\(〈0;a〉)": 5
  c: 〈0;("\(〈0;a〉)")〉
}
-- out/eval --
(struct){
  p: (struct){
  }
  "foo=bar": (string){ "str" }
  a: (string){ "str" }
  bb: (int){ 4 }
  b1: (int){ 4 }
  c: (int){ 5 }
  str: (int){ 5 }
}
