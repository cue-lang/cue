    }
  }
  f1: {
    "foo\(〈1;bar〉)": {
      "foo\(〈2;bar〉)baz": {
        name: "xx"
        a: 〈2;("foo\(〈1;bar〉)")〉["foo\(〈3;bar〉)baz"].name
        b: 〈1;("foo\(〈2;bar〉)baz")〉.name
      }
    }
  }
  c1: {
    "foo\(〈1;bar〉)": {
      [string]: {
        name: 〈1;-〉
