-- in.cue --
// Label aliases

// direct
a: [X=string]: X
a: bar: {}

// in struct
b: [X=string]: {X}
b: bar: {}

// in structs
c: [X=string]: X
c: [Y=string]: {{{Y}}}
c: bar: {}

// in sub field
d: [X=string]: name: X
d: bar: {}

// nested
e: [X=string]: [Y=string]: X + Y
e: foo: bar: {}

// Field aliases

bar: 3
f1: C="foo\(bar)": {
    name: "xx"
    foo: C.name
}

// nested
f1: D="foo\(bar)": E="foo\(bar)baz": {
    name: "xx"
    a: D["foo\(bar)baz"].name
    b: E.name
}

// Combo

c1: C="foo\(bar)": [D=string]: {
    name: D
    foo: C.x.name
}
c1: foo3: x: _


// TODO: support. Also not yet supported in old implementation.
// c10: {
// 	C=[string]: {
// 		name: "x"
// 		foo: C.name
// 	}
// }
// c2: c1 & { x: _ }

-- out/eval --
(struct){
  a: (struct){
    bar: (string){ "bar" }
  }
  b: (struct){
    bar: (string){ "bar" }
  }
  c: (struct){
    bar: (string){ "bar" }
  }
  d: (struct){
    bar: (struct){
      name: (string){ "bar" }
    }
  }
  e: (struct){
    foo: (struct){
      bar: (string){ "foobar" }
    }
  }
  bar: (int){ 3 }
  f1: (struct){
    foo3: (struct){
      name: (string){ "xx" }
      foo: (string){ "xx" }
      foo3baz: (struct){
        name: (string){ "xx" }
        a: (string){ "xx" }
        b: (string){ "xx" }
      }
    }
  }
  c1: (struct){
    foo3: (struct){
      x: (struct){
        name: (string){ "x" }
        foo: (string){ "x" }
      }
    }
  }
}
-- out/compile --
