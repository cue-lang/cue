      〈1;-〉
    }
  }
  b: {
    bar: {}
  }
  c: {
    [string]: 〈0;-〉
  }
  c: {
    [string]: {
      {
        {
          〈3;-〉
        }
      }
    }
  }
  c: {
    bar: {}
  }
  d: {
    [string]: {
      name: 〈1;-〉
    }
  }
  d: {
    bar: {}
  }
  e: {
    [string]: {
      [string]: (〈1;-〉 + 〈0;-〉)
    }
  }
  e: {
    foo: {
      bar: {}
    }
  }
  bar: 3
  f1: {
    "foo\(〈1;bar〉)": {
      name: "xx"
