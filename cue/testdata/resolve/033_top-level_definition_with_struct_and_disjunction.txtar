#name: top-level definition with struct and disjunction
#evalPartial
-- in.cue --
#def: {
	Type: string
	Text: string
	Size: int
}

#def: {
	Type: "B"
	Size: 0
} | {
	Type: "A"
	Size: 1
}
-- out/compile --
--- in.cue
{
  #def: {
    Type: string
    Text: string
    Size: int
  }
  #def: ({
    Type: "B"
    Size: 0
  }|{
    Type: "A"
    Size: 1
  })
}
-- out/eval --
(struct){
  #def: (#struct){ |((#struct){
      Type: (string){ "B" }
      Text: (string){ string }
      Size: (int){ 0 }
    }, (#struct){
      Type: (string){ "A" }
      Text: (string){ string }
      Size: (int){ 1 }
    }) }
}
