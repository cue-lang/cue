	b: {
		x: [int, ...string]
		result: strings.Join(x, " ")
	}

	c: {
		x: []
		result: strings.Join(x, int)
	}
}

decimalListErrors: {
	a: {
		x: [string]
		result: list.Avg(x)
	}

	b: {
		x: [int, ...string]
		result: list.Avg(x)
	}
}
