[
	{
		"description": "single dependency",
		"schema": {
			"$schema": "https://json-schema.org/draft/2019-09/schema",
			"dependentSchemas": {
				"bar": {
					"properties": {
						"foo": {
							"type": "integer"
						},
						"bar": {
							"type": "integer"
						}
					}
				}
			}
		},
		"skip": {
			"v3-roundtrip": "generation only supported in version https://json-schema.org/draft/2020-12/schema"
		},
		"tests": [
			{
				"description": "valid",
				"data": {
					"foo": 1,
					"bar": 2
				},
				"valid": true,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "no dependency",
				"data": {
					"foo": "quux"
				},
				"valid": true,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "wrong type",
				"data": {
					"foo": "quux",
					"bar": 2
				},
				"valid": false,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "wrong type other",
				"data": {
					"foo": 2,
					"bar": "quux"
				},
				"valid": false,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "wrong type both",
				"data": {
					"foo": "quux",
					"bar": "quux"
				},
				"valid": false,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "ignores arrays",
				"data": [
					"bar"
				],
				"valid": true,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "ignores strings",
				"data": "foobar",
				"valid": true,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "ignores other non-objects",
				"data": 12,
				"valid": true,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			}
		]
	},
	{
		"description": "boolean subschemas",
		"schema": {
			"$schema": "https://json-schema.org/draft/2019-09/schema",
			"dependentSchemas": {
				"foo": true,
				"bar": false
			}
		},
		"skip": {
			"v3-roundtrip": "generation only supported in version https://json-schema.org/draft/2020-12/schema"
		},
		"tests": [
			{
				"description": "object with property having schema true is valid",
				"data": {
					"foo": 1
				},
				"valid": true,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "object with property having schema false is invalid",
				"data": {
					"bar": 2
				},
				"valid": false,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "object with both properties is invalid",
				"data": {
					"foo": 1,
					"bar": 2
				},
				"valid": false,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "empty object is valid",
				"data": {},
				"valid": true,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			}
		]
	},
	{
		"description": "dependencies with escaped characters",
		"schema": {
			"$schema": "https://json-schema.org/draft/2019-09/schema",
			"dependentSchemas": {
				"foo\tbar": {
					"minProperties": 4
				},
				"foo'bar": {
					"required": [
						"foo\"bar"
					]
				}
			}
		},
		"skip": {
			"v3-roundtrip": "generation only supported in version https://json-schema.org/draft/2020-12/schema"
		},
		"tests": [
			{
				"description": "quoted tab",
				"data": {
					"foo\tbar": 1,
					"a": 2,
					"b": 3,
					"c": 4
				},
				"valid": true,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "quoted quote",
				"data": {
					"foo'bar": {
						"foo\"bar": 1
					}
				},
				"valid": false,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "quoted tab invalid under dependent schema",
				"data": {
					"foo\tbar": 1,
					"a": 2
				},
				"valid": false,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "quoted quote invalid under dependent schema",
				"data": {
					"foo'bar": 1
				},
				"valid": false,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			}
		]
	},
	{
		"description": "dependent subschema incompatible with root",
		"schema": {
			"$schema": "https://json-schema.org/draft/2019-09/schema",
			"properties": {
				"foo": {}
			},
			"dependentSchemas": {
				"foo": {
					"properties": {
						"bar": {}
					},
					"additionalProperties": false
				}
			}
		},
		"skip": {
			"v3-roundtrip": "generation only supported in version https://json-schema.org/draft/2020-12/schema"
		},
		"tests": [
			{
				"description": "matches root",
				"data": {
					"foo": 1
				},
				"valid": false,
				"skip": {
					"v3": "unexpected success",
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "matches dependency",
				"data": {
					"bar": 1
				},
				"valid": true,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "matches both",
				"data": {
					"foo": 1,
					"bar": 2
				},
				"valid": false,
				"skip": {
					"v3": "unexpected success",
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "no dependency",
				"data": {
					"baz": 1
				},
				"valid": true,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			}
		]
	}
]
