[
	{
		"description": "evaluating the same schema location against the same data location twice is not a sign of an infinite loop",
		"schema": {
			"$schema": "https://json-schema.org/draft/2019-09/schema",
			"$defs": {
				"int": {
					"type": "integer"
				}
			},
			"allOf": [
				{
					"properties": {
						"foo": {
							"$ref": "#/$defs/int"
						}
					}
				},
				{
					"additionalProperties": {
						"$ref": "#/$defs/int"
					}
				}
			]
		},
		"skip": {
			"v3-roundtrip": "generation only supported in version https://json-schema.org/draft/2020-12/schema"
		},
		"tests": [
			{
				"description": "passing case",
				"data": {
					"foo": 1
				},
				"valid": true,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			},
			{
				"description": "failing case",
				"data": {
					"foo": "a string"
				},
				"valid": false,
				"skip": {
					"v3-roundtrip": "could not extract schema"
				}
			}
		]
	}
]
