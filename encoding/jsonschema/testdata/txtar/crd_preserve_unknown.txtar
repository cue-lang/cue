                                    },
                                    "pruning": {
                                        "type": "object"
                                    },
                                    "pruning2": {
                                        "type": "object",
                                        "additionalProperties": {
                                            "type": "object",
                                            "properties": {
                                                "preserved": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            },
                            "pruning": {
                                "type": "object",
                                "properties": {
                                    "preserving": {
                                        "type": "object",
