-- in.cue --
first: {
	out: [1, 2]
}
ignore: {
	x: 1
	y: a.c
}
middle: {
	for x in first.out {
		ignore.y

		"la\(x)": ignore & {
			seq: x + 1
			val: "foo\(x)"
			out: ignore.x
		}
	}
}

a: {
	b: [ for x in middle {x}]
	c: {}
}
-- out/dependencies/field --
21: middle: middle
-- out/dependencies/all --
21: middle: middle
-- out/dependencies/dynamic --
21: middle: middle
21: x:      middle.la1
21: x:      middle.la2
