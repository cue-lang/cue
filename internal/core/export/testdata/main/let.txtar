unresolvedDisjunction: {
	#TypePrimitive: {
		_args: {
			required: bool
		}
		let Args = _args

		{"*": {}} |
		{"bool": #TypeBool & {_args: required: Args.required}}
	}

	#TypeBool: {
		_args: required: bool
		let Args = _args

		if !Args.required {
			// `default` sets the default value.
			default: bool | null
		}
	}
}
