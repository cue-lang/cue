-- in.cue --
import "net"

fail: {
    t1: net.ToIP4(1 | 2)
    t2: net.ToIP4(string)
}

ok: {
    t1: net.ToIP4(*"4.4.4.4" | "8.8.8.8")
}
-- out/net --
import "net"

fail: {
	t1: net.ToIP4(1 | 2)
	t2: net.ToIP4(string)
}
ok: {
	t1: [4, 4, 4, 4]
}
-- out/net-v3 --
import "net"

fail: {
	t1: net.ToIP4(1 | 2)
	t2: net.ToIP4(string)
}
ok: {
	t1: [4, 4, 4, 4]
}
