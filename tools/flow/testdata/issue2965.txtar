#IgnoreConcrete: true
#InferTasks: true
-- in.cue --
package x

root: {
	prepare: {
		$id: "prepare"
		stdout: string
	}
	env2: {
		// Note: the string interpolation here is the only difference
		// from issue2517.txtar. It makes the value incomplete
		// rather than just non-concrete.
		input: "\(prepare.stdout)"
	}
	run: {
		$id: "run"
		env: env2
	}
}
