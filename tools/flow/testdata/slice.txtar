	{
		$id: "valToOut"
		val: "foo"
		out: string
	},
	{
		$id:    "valToOut"
		$after: root[0]
		val:    "bar"
		out:    string
	},
	{
		$id: "valToOut"
		out: root[0].out + root[1].out
	},
]

